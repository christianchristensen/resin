<taglib>
  <tlib-version>1.0</tlib-version>
  <jsp-version>2.0</jsp-version>
  <short-name>message</short-name>
  <tag>
    <name>message</name>
    <tag-class>example.taglib.MessageTag</tag-class>
    <attribute>
      <name>title</name>
      <required>false</required>
    </attribute>
  </tag>
  <tag>
    <name>add</name>
    <tag-class>example.taglib.MessageAddTag</tag-class>
    <attribute>
      <name>text</name>
      <required>true</required>
    </attribute>
  </tag>
</taglib>
